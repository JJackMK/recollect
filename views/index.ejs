<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/styles/style.css" />
  </head>
  <body class="bg-purple-900">
    <% if (typeof userId !== 'undefined' && userId) { %>
      <%- include('partials/navbar_loggedin', {username: username}) %>
      <% } else { %>
      <%- include('partials/navbar') %>
    <% } %>
    <div class="mt-8 flex justify-center">
      <div class="relative w-8/12 sm:w-2/3 md:w-2/3 lg:w-2/5 xl:w-1/3 justify-center">
        <input
          type="text"
          class="w-full h-7 pr-10 pl-5 rounded-full z-0 focus:shadow focus:outline-none"
          placeholder="Search"
        />
    
        <div class="absolute top-4 right-3">
          <i class="fa fa-search text-gray-400 z-20 hover:text-gray-500"></i>
        </div>
      </div>
    </div>
    <%- include('partials/hero.ejs') %>
    <%- include('partials/top_rated_collections.ejs') %>
    

    
  </body>
  
</html>
